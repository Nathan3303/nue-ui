{
    "name": "@nue-ui/repo",
    "version": "0.2.4",
    "description": "NueUI repo",
    "author": "Nathan Lee",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "test": "echo \"NueUI Test\"",
        "lint": "npx eslint -c eslint.config.mjs",
        "lint-on-debug": "npx eslint -c eslint.config.mjs --debug",
        "lint:lint-staged": "lint-staged",
        "format": "prettier --write --config .prettierrc .",
        "prepare": "husky",
        "core": "pnpm --filter nue-ui",
        "document": "pnpm --filter @nue-ui/document",
        "playground": "pnpm --filter @nue-ui/playground",
        "histoire": "pnpm --filter @nue-ui/histoire",
        "shadlike-theme": "pnpm --filter nue-ui-theme-shadlike",
        "iconfont": "pnpm --filter nue-ui-iconfont",
        "build": "pnpm core build && pnpm shadlike-theme build && pnpm iconfont build"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Nathan3303/nue-ui"
    },
    "keywords": [
        "nue-ui",
        "nue",
        "ui",
        "components"
    ],
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.19.1",
        "@types/shelljs": "^0.8.16",
        "@typescript-eslint/eslint-plugin": "^8.34.1",
        "@typescript-eslint/parser": "^8.34.1",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vitejs/plugin-vue-jsx": "^4.2.0",
        "@vue/tsconfig": "^0.5.1",
        "cross-env": "^7.0.3",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.5.0",
        "eslint-plugin-vue": "^9.33.0",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.6",
        "postcss-color-mix": "^1.1.0",
        "postcss-each": "^1.1.0",
        "postcss-each-variables": "^0.3.0",
        "postcss-for": "^2.1.1",
        "postcss-nested": "^6.2.0",
        "prettier": "^3.5.3",
        "shelljs": "^0.8.5",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": "6.3.4",
        "vitest": "3.0.5",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "nue-ui": "workspace:*",
        "nue-ui-theme-shadlike": "workspace:*",
        "nue-ui-iconfont": "workspace:*",
        "@nue-ui/components": "workspace:*",
        "@nue-ui/hooks": "workspace:*",
        "@nue-ui/utils": "workspace:*",
        "@nue-ui/themes": "workspace:*",
        "vue": "^3.5.17",
        "lodash-es": "^4.17.21"
    }
}