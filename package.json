{
    "name": "@nue-ui/repo",
    "version": "0.1.0",
    "description": "NueUI repo",
    "author": "Nathan Lee",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "test": "echo \"todo\"",
        "lint": "npx eslint -c eslint.config.mjs",
        "lint-on-debug": "npx eslint -c eslint.config.mjs --debug",
        "lint:lint-staged": "lint-staged",
        "format": "prettier --write --config .prettierrc .",
        "prepare": "husky",
        "core": "pnpm --filter nue-ui",
        "document": "pnpm --filter @nue-ui/document",
        "playground": "pnpm --filter @nue-ui/playground",
        "histoire": "pnpm --filter @nue-ui/histoire"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nathanlee323/nue-ui.git"
    },
    "keywords": [
        "nue-ui",
        "nue",
        "nuxt",
        "ui",
        "components"
    ],
    "bugs": {},
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.11.20",
        "@types/shelljs": "^0.8.15",
        "@typescript-eslint/eslint-plugin": "^8.12.2",
        "@typescript-eslint/parser": "^8.12.2",
        "@vitejs/plugin-vue": "^5.1.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/tsconfig": "^0.5.1",
        "cross-env": "^7.0.3",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.30.0",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "postcss-color-mix": "^1.1.0",
        "postcss-each": "^1.1.0",
        "postcss-each-variables": "^0.3.0",
        "postcss-for": "^2.1.1",
        "postcss-nested": "^6.0.1",
        "prettier": "^3.3.3",
        "shelljs": "^0.8.5",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.12.2",
        "vue": "^3.2.37",
        "vite": "6.3.4",
        "vitest": "3.0.5",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "@nue-ui/components": "workspace:*",
        "@nue-ui/hooks": "workspace:*",
        "@nue-ui/theme": "workspace:*",
        "@nue-ui/utils": "workspace:*",
        "nue-ui": "workspace:*",
        "lodash-es": "^4.17.21"
    }
}